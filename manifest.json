{
    "manifest_version": 3,
    "name": "AWS Role Name Changer",
    "version": "0.0.2",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://*.aws.amazon.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  